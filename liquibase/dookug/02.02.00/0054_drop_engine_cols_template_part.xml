<!DOCTYPE defaultProperties SYSTEM "common/params.dtd">
    <databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">

    <!-- include the default properties -->
    &propertiesAll;

   <changeSet id="0054" author="jozsef.holczer" labels="02.02.00" >
        <comment>drop template_engine, generator_engine cols from template_part.</comment>
    
        <dropColumn 
            schemaName="${schema_name}"
            tableName="template_part">
            <column name="template_engine"/>
            <column name="generator_engine"/>
        </dropColumn>
      
   </changeSet>

</databaseChangeLog>